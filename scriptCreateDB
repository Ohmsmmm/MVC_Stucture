CREATE DATABASE ohmDB

CREATE TABLE ohmDB.dbo.PATIENT_COVID_STATUS (
	HNID varchar(4) NULL PRIMARY KEY,
	COVID_19_Status varchar(15) COLLATE Thai_CI_AS NULL,
)

CREATE TABLE ohmDB.dbo.PATIENTS (
	HNID varchar(4) NOT NULL PRIMARY KEY,
	Firstname varchar(30)  NULL,
	Lastname varchar(30)  NULL,
	HID varchar(4)  NULL
)

CREATE TABLE ohmDB.dbo.HOSPITALS (
	HID bigint IDENTITY(1,1) NOT NULL PRIMARY KEY,
	Title varchar(30)  NULL,
)
